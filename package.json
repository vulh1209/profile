{
  "name": "profile",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate-seo && tsc -b && vite build && npm run fix-mime",
    "build:gh": "npm run generate-seo && tsc -b && vite build && npm run fix-mime",
    "fix-mime": "node scripts/fix-mime-types.js",
    "generate-seo": "node scripts/generate-sitemap.js",
    "seo": "npm run generate-seo",
    "png-to-svg": "node scripts/png-to-svg.js",
    "convert-images": "npm run png-to-svg",
    "generate-cv": "node scripts/generate-cv-pdf.js",
    "generate-cv-prod": "node scripts/generate-cv-pdf.js --production",
    "cv": "npm run generate-cv",
    "cv-prod": "npm run generate-cv-prod",
    "build-with-cv": "npm run generate-cv && npm run build",
    "precommit": "npm run generate-cv-prod",
    "test-precommit": "node scripts/test-precommit.js",
    "prepare": "husky install",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "framer-motion": "^12.19.1",
    "lucide-react": "^0.523.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "puppeteer": "^24.11.2",
    "tailwindcss": "^3.4.17",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  }
}
